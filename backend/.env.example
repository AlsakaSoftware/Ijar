# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Apple Push Notifications (APNs) Configuration
# Option 1: Use p8 auth key (recommended)
APN_AUTH_KEY=your_p8_key_content
APN_KEY_ID=your_key_id
APN_TEAM_ID=your_team_id

# Option 2: Use certificate files
# APN_CERT_PATH=path/to/cert.pem
# APN_KEY_PATH=path/to/key.pem
# APN_PASSPHRASE=optional_passphrase

# App Bundle ID
APN_BUNDLE_ID=com.yourapp.ijar

# Environment
NODE_ENV=development

# Optional: GitHub Actions will set these automatically
GITHUB_ACTIONS=false